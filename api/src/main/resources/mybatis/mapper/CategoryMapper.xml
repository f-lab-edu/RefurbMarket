<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.refurbmarket.repository.mapper.CategoryMapper">
    <select id="findAll" resultType="com.refurbmarket.domain.Category">
        SELECT id, depth, parentId, name
        FROM Category
